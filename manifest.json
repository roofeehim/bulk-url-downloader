
{
  "manifest_version": 3,
  "name": "Bulk URL Downloader",
  "version": "1.0",
  "description": "Download files from a list of URLs.",
  "permissions": [
    "downloads",
    "tabs",
    "activeTab"
  ],
  "icons": {
    "16": "favicon.ico",
    "48": "favicon.ico",
    "128": "favicon.ico"
  },
  "action": {
    "default_icon": "favicon.ico",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
